{
  "name": "plk-api2ts",
  "version": "1.0.24",
  "description": "",
  "main": "index.js",
  "scripts": {
    "fetch": "node ./lib/createData.js",
    "create": "node ./lib/createTs.js",
    "init": "npm run fetch && npm run create",
    "src:fetch": "ts-node ./src/main.ts --type=createData",
    "src:create": "ts-node ./src/main.ts --type=createTs",
    "src:init": "ts-node ./src/main.ts --platform=dev",
    "case": "ts-node ./src/main.ts --type=createTs --target=./case",
    "update:link": "tsc && npm link",
    "build": "tsc -d",
    "createDataFilter": "ts-node ./src/main.ts --type=createData --filterPath=/app-mobile-web/api/app/mobile/andon/export",
    "createTsFilter": "ts-node ./src/main.ts --type=createTs --filterPath=/app-mobile-web/api/app/mobile/andon/export",
    "createFilter": "ts-node ./src/main.ts --filterPath=/app-mobile-web/api/app/mobile/andon/export"
  },
  "repository": "git+https://github.com/HarryPoint/plk-api2ts.git",
  "bugs": {
    "url": "https://github.com/HarryPoint/plk-api2ts/issues"
  },
  "homepage": "https://github.com/HarryPoint/plk-api2ts#readme",
  "keywords": [
    "Swagger",
    "typescript",
    "api2ts",
    "Swagger2",
    "code generation",
    "code refactor"
  ],
  "author": "harry",
  "license": "ISC",
  "bin": {
    "api2json": "./bin/api2json",
    "api2ts": "./bin/api2ts"
  },
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "lodash": "^4.17.21",
    "qs": "^6.11.2",
    "ts-morph": "^20.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.15",
    "@types/qs": "^6.9.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
