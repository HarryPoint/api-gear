{
  "name": "api-gear",
  "version": "0.0.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lib": "node ./lib/main.js",
    "lib:platform": "node ./lib/main.js --platform=dev",
    "lib:json": "node ./lib/main.js --json=true",
