  "paths": {
    "/app-mobile-web/api/app/mobile/qualityTask/report": {
      "post": {
        "tags": [
          "质检任务相关"
        ],
        "summary": "生产任务-上报反馈",
        "operationId": "reportUsingPOST_1",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "handleDTO",
            "description": "handleDTO",
            "required": true,
            "schema": {
              "originalRef": "生产处理",
              "$ref": "#/definitions/生产处理"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "originalRef": "JSONResult«object»",
              "$ref": "#/definitions/JSONResult«object»"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false,
        "x-order": "85"
      }
    }
  },
  "definitions": {
    "生产处理": {
      "type": "object",
      "required": [
        "produceAbnormalCategoryId",
        "produceTaskId"
      ],
      "properties": {
        "produceTaskId": {
          "type": "integer",
          "format": "int64",
          "description": "生产任务id，任务外的上报反馈可不传"
        },
        "produceAbnormalCategoryId": {
          "type": "integer",
          "format": "int64",
          "description": "生产异常类型id"
        },
        "tagIds": {
          "type": "array",
          "description": "标签id集合",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        },
        "imageKeys": {
          "type": "array",
          "description": "图片key集合",
          "items": {
            "type": "string"
          }
        },
        "description": {
          "type": "string",
          "description": "描述"
        },
        "backProduceTechnologyRoutingStepId": {
          "type": "integer",
          "format": "int64",
          "description": "返工工艺路径步骤id，返工逻辑必填"
        },
        "lotOrderId": {
          "type": "integer",
          "format": "int64",
          "description": "对应批次id，除了上报，均必填"
        },
        "wipRpId": {
          "type": "integer",
          "format": "int64",
          "description": "wip状态记录id，除了上报，均必填"
        },
        "abnormalQuantity": {
          "type": "number",
          "description": "异常数量，报废/返工/暂扣时必填"
        },
        "backProduceType": {
          "type": "string",
          "description": "返工生产后处理方式，返工逻辑必填",
          "enum": [
            "  RETURN-返回原工序",
            "  ORDER-顺序生产"
          ]
        }
      },
      "title": "生产处理"
    },
    "JSONResult«object»": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32",
          "example": 0,
          "description": "返回码"
        },
        "msg": {
          "type": "string",
          "example": "服务器异常",
          "description": "返回消息说明"
        },
        "data": {
          "type": "object",
          "description": "响应结果"
        },
        "ts": {
          "type": "integer",
          "format": "int64",
          "example": 1356019200000,
          "description": "服务器结果返回时的 Unix timestamp,单位毫秒"
        }
      },
      "title": "JSONResult«object»"
    }
  }
