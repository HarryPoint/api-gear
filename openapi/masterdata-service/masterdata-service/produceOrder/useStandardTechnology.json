        "x-order": "300"
      }
    }
  },
  "definitions": {
    "标准工艺使用请求": {
      "type": "object",
      "properties": {
        "itemList": {
          "type": "array",
          "description": "生产订单和标准工艺关联关系列表",
          "items": {
            "originalRef": "生产订单和标准工艺关系请求",
            "$ref": "#/definitions/生产订单和标准工艺关系请求"
          }
        }
      },
      "title": "标准工艺使用请求"
    },
    "生产订单和标准工艺关系请求": {
      "type": "object",
      "properties": {
        "produceOrderId": {
          "type": "integer",
          "format": "int64",
          "description": "生产订单ID"
        },
        "standardTechnologyId": {
          "type": "integer",
          "format": "int64",
          "description": "标准工艺ID"
        },
        "childMaterialProduceOrderConfigList": {
          "type": "array",
          "description": "子物料生产订单配置请求",
          "items": {
            "originalRef": "子生产订单配置请求",
            "$ref": "#/definitions/子生产订单配置请求"
          }
        },
        "notProduceChildMaterialStandardTechnologyNodeIdList": {
          "type": "array",
          "description": "不需要生产子物料的生产工艺节点ID",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "title": "生产订单和标准工艺关系请求"
    },
    "子生产订单配置请求": {
      "type": "object",
      "required": [
        "endTime",
        "lotOrders",
        "priorityLevel",
        "totalCount"
      ],
      "properties": {
        "standardTechnologyNodeId": {
          "type": "integer",
          "format": "int64",
          "description": "标准工艺路径，节点ID"
        },
        "priorityLevel": {
          "type": "integer",
          "format": "int32",
          "description": "订单优先级，数值越大优先级越高"
        },
        "totalCount": {
          "type": "number",
          "description": "总生产数量"
        },
        "beginTime": {
          "type": "string",
          "description": "计划开始时间 yyyy-MM-dd HH:mm:ss"
        },
        "endTime": {
          "type": "string",
          "description": "计划结束时间 yyyy-MM-dd HH:mm:ss"
        },
        "productionLineId": {
          "type": "integer",
          "format": "int64",
          "description": "产线"
        },
        "lotOrders": {
          "type": "array",
          "description": "批次信息列表",
          "items": {
            "originalRef": "批次编辑DTO",
            "$ref": "#/definitions/批次编辑DTO"
          }
        }
      },
      "title": "子生产订单配置请求"
    },
    "批次编辑DTO": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64",
          "description": "id"
        },
        "lotNo": {
          "type": "string",
          "description": "批次号"
        },
        "lotName": {
          "type": "string",
          "description": "批次名称"
        },
        "totalCount": {
          "type": "number",
          "description": "总数量"
        }
      },
      "title": "批次编辑DTO"
    },
    "JSONResult«long»": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32",
          "example": 0,
          "description": "返回码"
        },
        "msg": {
          "type": "string",
          "example": "服务器异常",
          "description": "返回消息说明"
        },
        "data": {
          "type": "integer",
          "format": "int64",
          "description": "响应结果"
        },
        "ts": {
          "type": "integer",
          "format": "int64",
          "example": 1356019200000,
          "description": "服务器结果返回时的 Unix timestamp,单位毫秒"
        }
      },
      "title": "JSONResult«long»"
    }
  }
}