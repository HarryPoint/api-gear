      "post": {
        "tags": [
          "生产订单相关"
        ],
        "summary": "使用生产工艺",
        "operationId": "useStandardTechnologyUsingPOST",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "useRequest",
            "description": "useRequest",
            "required": true,
            "schema": {
              "originalRef": "标准工艺使用请求",
              "$ref": "#/definitions/标准工艺使用请求"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "originalRef": "JSONResult«long»",
              "$ref": "#/definitions/JSONResult«long»"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
