  "paths": {
    "/masterdata-service/qualityInspectionPlan/edit": {
      "post": {
        "tags": [
          "质检方案"
        ],
        "summary": "新增/编辑质检方案",
        "operationId": "editUsingPOST_18",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "edit",
            "description": "edit",
            "required": true,
            "schema": {
              "originalRef": "编辑质检方案 DTO",
              "$ref": "#/definitions/编辑质检方案 DTO"
            }
          },
          {
            "name": "enterpriseId",
            "in": "query",
            "description": "enterpriseId",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "userId",
            "in": "query",
            "description": "userId",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "originalRef": "JSONResult«object»",
              "$ref": "#/definitions/JSONResult«object»"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false,
        "x-order": "30"
      }
    }
  },
  "definitions": {
    "编辑质检方案 DTO": {
      "type": "object",
      "required": [
        "isCodeRule",
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64",
          "description": "id，如果不传就是新增"
        },
        "name": {
          "type": "string",
          "description": "质检方案名"
        },
        "code": {
          "type": "string",
          "description": "质检方案编码"
        },
        "isCodeRule": {
          "type": "string",
          "description": "是否应用编码规则",
          "enum": [
            "  Y-是",
            "  N-否"
          ]
        },
        "qualityInspectionCategoryId": {
          "type": "integer",
          "format": "int64",
          "description": "所属质检分类id"
        },
        "itemList": {
          "type": "array",
          "description": "质检项清单",
          "items": {
            "originalRef": "质检方案质检项关联信息编辑DTO",
            "$ref": "#/definitions/质检方案质检项关联信息编辑DTO"
          }
        },
        "issueCode": {
          "type": "string",
          "description": "版次号"
        },
        "remark": {
          "type": "string",
          "description": "备注"
        },
        "attachedFileKey": {
          "type": "string",
          "description": "附件文件key"
        },
        "type": {
          "type": "string",
          "description": "质检方式",
          "enum": [
            "  ALL-全检",
            "  RADIO_SPOT_CHECK-比例抽检",
            "  FIX_SPOT_CHECK-固定抽检"
          ]
        },
        "qualityInspectionRatio": {
          "type": "number",
          "description": "质检比例（填多少传多少）"
        },
        "qualityInspectionQuantity": {
          "type": "number",
          "description": "质检数量"
        }
      },
      "title": "编辑质检方案 DTO"
    },
    "质检方案质检项关联信息编辑DTO": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64",
          "description": "关联id"
        },
        "qualityInspectionItemId": {
          "type": "integer",
          "format": "int64",
          "description": "所属质检项id"
        },
        "numberStandard": {
          "type": "string",
          "description": "数值质检标准",
          "enum": [
            "  BETWEEN-数值区间",
            "  GT-数值大于",
            "  GE-数值大于等于",
            "  LT-数值小于",
            "  LE-数值小于等于"
          ]
        },
        "compareNumber": {
          "type": "number",
          "description": "比较数值"
        },
        "lowerLimit": {
          "type": "number",
          "description": "下限"
        },
        "upperLimit": {
          "type": "number",
          "description": "上限"
        },
        "numberUnit": {
          "type": "string",
          "description": "数值单位"
        }
      },
      "title": "质检方案质检项关联信息编辑DTO"
    },
    "JSONResult«object»": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32",
          "example": 0,
          "description": "返回码"
        },
        "msg": {
          "type": "string",
          "example": "服务器异常",
          "description": "返回消息说明"
        },
        "data": {
          "type": "object",
          "description": "响应结果"
        },
        "ts": {
          "type": "integer",
          "format": "int64",
          "example": 1356019200000,
          "description": "服务器结果返回时的 Unix timestamp,单位毫秒"
        }
      },
      "title": "JSONResult«object»"
    }
  }
}