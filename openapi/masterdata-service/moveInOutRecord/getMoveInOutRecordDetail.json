    "/masterdata-service/moveInOutRecord/getMoveInOutRecordDetail": {
      "get": {
        "tags": [
          "进出料记录管理"
        ],
        "summary": "根据进出记录查询进出记录详情",
        "description": "<p style='color:red'>注意：<br/>1、仅STAGING(暂存)可查<br/>2、进去查了详情要把记录id带上，在提交暂存或者提交进出料的时候要同时返回回来<br/>3、stepSetInfo.bomConsumeSureType == 'MOVE_IN' 时，出站页面不显示消耗物料右边让用户手动录入实际数的内容'<br/>4、stepSetInfo.bomConsumeSureType == 'MOVE_OUT' 时，进站页面不显示消耗物料右边让用户手动录入实际数的内容'<br/></p>",
        "operationId": "getMoveInOutRecordDetailUsingGET",
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "recordId",
            "in": "query",
            "description": "记录id",
            "required": true,
            "type": "string"
          },
          {
            "name": "enterpriseId",
            "in": "query",
            "description": "enterpriseId",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "originalRef": "JSONResult«进出站信息VO»",
              "$ref": "#/definitions/JSONResult«进出站信息VO»"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false,
        "x-order": "20"
      }
    }
  },
